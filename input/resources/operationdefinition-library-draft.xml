<?xml version="1.0" encoding="UTF-8"?>
<OperationDefinition xmlns="http://hl7.org/fhir">
  <id value="Library-package"/>
  <url value="http://hl7.org/fhir/uv/cmi/OperationDefinition/Library-draft"/>
  <version value="1.0.0"/>
  <name value="LibraryDrafting"/>
  <title value="Library Drafting"/>
  <status value="draft"/>
  <kind value="operation"/>
  <date value="2023-05-12"/>
  <publisher value="HL7 (FHIR Project)"/>
  <contact>
    <telecom>
      <system value="url"/>
      <value value="http://hl7.org/fhir"/>
    </telecom>
    <telecom>
      <system value="email"/>
      <value value="fhir@lists.hl7.org"/>
    </telecom>
  </contact>
  <description value="Creates a draft version of an asset-collection library such as a quality program release or version manifest, as well as for all resources it is composed of."/>
  <code value="draft"/>
  <comment value="This operation is used to set the status and version."/>
  <resource value="Library"/>
  <system value="false"/>
  <type value="true"/>
  <instance value="true"/>
  <parameter>
    <name value="id"/>
    <use value="in"/>
    <min value="0"/>
    <max value="1"/>
    <documentation value="The logical id of the library to draft. The server must know the library (e.g. it is defined explicitly in the server&#39;s libraries)"/>
    <type value="string"/>
  </parameter>
  <parameter>
    <name value="version"/>
    <use value="in"/>
    <min value="1"/>
    <max value="1"/>
    <documentation value="A semantic version in the form MAJOR.MINOR.PATCH"/>
    <type value="string"/>
  </parameter>
  <parameter>
    <name value="return"/>
    <use value="out"/>
    <min value="1"/>
    <max value="1"/>
    <documentation value="The new Library with the draft status and version."/>
    <type value="Library"/>
  </parameter>
</OperationDefinition>
